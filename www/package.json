{
	"name": "gp2040-configurator",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"dependencies": {
		"@hello-pangea/color-picker": "^3.2.2",
		"axios": "^1.3.5",
		"bootstrap": "^5.3.0-alpha3",
		"crypto-js": "^4.1.1",
		"formik": "^2.2.9",
		"i18next": "^23.1.0",
		"i18next-browser-languagedetector": "^7.0.2",
		"javascript-color-gradient": "^2.4.4",
		"jsencrypt": "^3.3.2",
		"lodash": "^4.17.21",
		"react": "^18.2.0",
		"react-beautiful-dnd": "^13.1.1",
		"react-bootstrap": "^2.7.4",
		"react-dom": "^18.2.0",
		"react-i18next": "^12.3.1",
		"react-router-dom": "^6.10.0",
		"yup": "^1.1.1"
	},
	"scripts": {
		"analyze": "vite build --sourcemap true && source-map-explorer 'build/assets/*.js' --no-border-checks",
		"build": "vite build && npm run makefsdata",
		"dev-server": "nodemon -r dotenv/config ./server/app.js",
		"dev": "concurrently --kill-others \"npm run dev-server\" \"npm start\"",
		"dev-board": "cross-env VITE_DEV_BASE_URL=http://192.168.7.1 npm start ",
		"format": "prettier --write \"{src,server}/**/*.{tsx,ts,jsx,js,json,scss}\"",
		"lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
		"makefsdata": "node makefsdata.js",
		"start": "vite"
	},
	"devDependencies": {
		"@types/node": "^20.5.9",
		"@vitejs/plugin-react": "^4.0.0",
		"concurrently": "^8.0.1",
		"cors": "^2.8.5",
		"cross-env": "^7.0.3",
		"dotenv": "^16.1.4",
		"eslint": "^8.38.0",
		"eslint-plugin-i18next": "^6.0.3",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.1",
		"express": "^4.18.2",
		"nodemon": "^2.0.22",
		"prettier": "^3.0.1",
		"sass": "^1.62.1",
		"source-map-explorer": "^2.5.3",
		"vite": "^4.3.9"
	}
}
